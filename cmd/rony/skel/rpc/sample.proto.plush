syntax = "proto3";
package rpc ;
import "github.com/ronaksoft/rony/options.proto";
option go_package = "<%= packagePath() %>/rpc";


// Sample
service Sample {
    option (rony_cobra_cmd) = true;
    rpc Echo(EchoRequest) returns (EchoResponse);
}

// EchoRequest
message EchoRequest {
    int64 ID = 1;
}

// Response1
message EchoResponse {
    int64 ReqID = 1;
    string RandomText = 2;
}